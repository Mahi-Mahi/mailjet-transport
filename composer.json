{
    "name": "mahi-mahi/mailjet-transport",
    "description": "MailJet Transport for Laravel 5.6",
    "keywords": ["mailjet", "laravel", "email"],
    "homepage": "https://github.com/mahi-mahi/mailjet-transport",
    "require": {
        "mailjet/mailjet-apiv3-php": "^1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Mourlevat",
            "email": "olivier.mourlevat@mahi-mahi.fr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "orchestra/testbench": "~3.6"
    },
    "autoload": {
        "psr-4": {
            "MahiMahi\\MailjetTransport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MahiMahi\\MailjetTransport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MahiMahi\\MailjetTransport\\MailjetTransportServiceProvider"
            ]
        }
    }
}
